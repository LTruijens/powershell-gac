<?xml version="1.0" encoding="utf-8"?>
<Types>
  <Type>
    <Name>System.Reflection.AssemblyName</Name>
    <Members>
      <ScriptProperty>
        <Name>PublicKeyToken</Name>
        <GetScriptBlock>
          ($this.GetPublicKeyToken() | ForEach-Object { '{0:x2}' -f $_ }) -join ''
        </GetScriptBlock>
      </ScriptProperty>
      <!-- DisplayName -->
    </Members>
  </Type>
</Types>
